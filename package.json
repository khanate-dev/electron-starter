		"@vitest/ui": "^1.2.0",
		"cross-env": "^7.0.3",
		"cspell": "^8.3.2",
		"css-loader": "^6.9.0",
		"electron": "^28.1.3",
		"electron-devtools-assembler": "^1.2.0",
		"electron-icon-builder": "^2.0.1",
