		"eslint-plugin-unused-imports": "^3.0.0",
		"eslint-plugin-vitest": "^0.3.20",
		"fork-ts-checker-webpack-plugin": "^9.0.2",
		"jsdom": "^23.2.0",
		"msw": "^2.0.14",
		"node-html-parser": "^6.1.12",
		"node-loader": "^2.0.0",
