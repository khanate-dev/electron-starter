{
	"name": "electron-starter",
	"productName": "electron-starter",
	"version": "1.0.0",
	"description": "Electron React Starter Application",
	"keywords": [],
	"author": {
		"name": "Khanate",
		"email": "dev@khanate.pk",
		"url": "https://khante.pk"
	},
	"main": ".webpack/main",
	"scripts": {
		"start": "cross-env DEV=1 electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"test": "vitest watch",
		"test-ui": "vitest --ui",
		"typecheck": "vitest typecheck",
		"lint": "eslint . --fix --report-unused-disable-directives && pnpm format && pnpm spellcheck",
		"format": "prettier --write --log-level silent .",
		"spellcheck": "cspell . --quiet",
		"generate-icons": "node ./scripts/generate-icons.ts",
		"generate-favicons": "electron-icon-builder --input=./src/app/assets/favicon/favicon.png --output=./src/app/assets/favicon",
		"audit": "electronegativity -i ./",
		"diag": "tsc --extendedDiagnostics",
		"trace": "tsc --generateTrace ./trace && npx @typescript/analyze-trace trace"
	},
	"dependencies": {
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.15.4",
		"@mui/material": "^5.15.4",
		"@mui/x-date-pickers": "^6.19.0",
		"dayjs": "^1.11.10",
		"electron-squirrel-startup": "^1.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hot-toast": "^2.4.1",
		"react-router-dom": "^6.21.2",
		"serialport": "^12.0.0",
		"xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@doyensec/electronegativity": "^1.10.3",
		"@electron-forge/cli": "^7.2.0",
		"@electron-forge/maker-deb": "^7.2.0",
		"@electron-forge/maker-squirrel": "^7.2.0",
		"@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
		"@electron-forge/plugin-webpack": "^7.2.0",
		"@electron-forge/publisher-github": "^7.2.0",
		"@mui/system": "^5.15.4",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
		"@testing-library/jest-dom": "^6.2.0",
		"@testing-library/react": "^14.1.2",
		"@testing-library/user-event": "^14.5.2",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "^20.11.0",
		"@types/prettier": "^3.0.0",
