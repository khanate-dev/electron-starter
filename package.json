		"@types/react-dom": "^18.2.18",
		"@types/react-router-dom": "^5.3.3",
		"@types/testing-library__jest-dom": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@typescript-eslint/parser": "^6.18.1",
		"@typescript/analyze-trace": "^0.10.1",
